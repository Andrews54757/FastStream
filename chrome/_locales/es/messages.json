    "welcome_page_usage_header": {
        "message": "Cómo Utilizar"
    },
    "welcome_page_usage_content0": {
        "message": "Hay cuatro formas de usar FastStream"
    },
    "welcome_page_usage_content1": {
        "message": "Puedes ir a un sitio web con una transmisión de video y activar FastStream haciendo clic en el icono. Reproduce la transmisión y el cliente se reemplazará automáticamente con FastStream"
    },
    "welcome_page_usage_content2": {
        "message": "Puedes ir a una nueva pestaña y presionar el ícono de la extensión para ir al reproductor."
    },
    "welcome_page_usage_content3": {
        "message": "Si tienes la opción habilitada, ve a una url mp4\/mpd\/m3u8 y el reproductor la reproducirá."
    },
    "welcome_page_usage_content4": {
        "message": "Mantén el reproductor abierto en una nueva pestaña y recogerá fuentes mientras navegas en otros lugares."
    },
    "welcome_page_usage_end": {
        "message": "Gracias por instalar FastStream! Háganos saber si tiene algún problema o sugerencia."
    },
