    "perms_page_title": {
        "message": "Autorizzazioni FastStream"
    },
    "perms_page_about_header": {
        "message": "Informazioni sulle autorizzazioni richieste da FastStream"
    },
    "perms_page_about_content0": {
        "message": "Affinché l'estensione funzioni, deve poter disporre di determinate autorizzazioni. Queste autorizzazioni vengono utilizzate solo per le funzionalità di base."
    },
    "perms_page_about_content1": {
        "message": "FastStream non raccoglie alcun dato da te. Non ti traccia. Non invia alcun dato a nessun server. Non ha nemmeno un server a cui inviare dati."
    },
    "perms_page_about_content2": {
        "message": "Non ti fidi di me? FastStream ha la sua fonte disponibile su"
    },
    "perms_page_about_content3": {
        "message": "Puoi esaminare il codice da solo o installare manualmente la tua build personalizzata seguendo le istruzioni."
    },
    "perms_page_breakdown_header": {
        "message": "Ripartizione delle autorizzazioni"
